{
  "manifest_version": 2,
  "name": "RCP (Right-Click Prompt): AI Prompt Manager - TEST",
  "version": "1.29.0-test",
  "description": "Test version with debugging enabled",
  "homepage_url": "https://rightclickprompt.com",
  "author": "RCP Team",
  "icons": {
    "16": "src/assets/icon16.png",
    "48": "src/assets/icon48.png",
    "128": "src/assets/icon128.png"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardWrite",
    "activeTab",
    "scripting"
  ],
  "background": {
    "scripts": ["preloaded-prompts-library.js", "background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "src/assets/icon16.png",
      "48": "src/assets/icon48.png",
      "128": "src/assets/icon128.png"
    },
    "default_popup": "popup-test.html",
    "default_title": "RCP - Right Click Prompt (TEST)"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "assets/floatingIcon.ts-CJpmuZwW.js",
    "category-selector.js",
    "full-ui.html",
    "full-ui.js"
  ],
  "applications": {
    "gecko": {
      "id": "rcp-firefox-test@example.com",
      "strict_min_version": "78.0"
    }
  }
}